package main

import "fmt"

func main() {
	str1 := "This // is not a comment inside string"
	str2 := "URL: https://example.com"

	url := "https://api.example.com/v1/users"
	comment := "Use // for line comments in Go"

	escaped := "He said \"// is for comments\" to me"

	char := '/'
	doubleSlash := '\\'

	mixed := "Single quote: ' and // inside"

	fmt.Println(str1, str2, url, comment, escaped, char, doubleSlash, mixed)
}
